{
    "manifest_version": 3,
    "name": "Custom New Tab",
    "version": "1.0",
    "description": "Your Task viewer",
    "icons": {
      "16": "img/check-circle_16.png",
      "48": "img/check-circle_48.png",
      "128": "img/check-circle_128.png"
    },
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    },
    "permissions": [
        "tabs",
        "scripting"
      ],
      "host_permissions": ["http://*/*", "https://*/*"]
  }


 
  